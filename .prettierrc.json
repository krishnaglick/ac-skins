{
    "arrowParens": "avoid",
    "printWidth": 120,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "jsxBracketSameLine": false,
    "endOfLine": "auto",
    "overrides": [{
        "files": [
            "*.ts",
            "*.tsx"
        ],
        "options": {
            "parser": "typescript"
        }
    }]
}